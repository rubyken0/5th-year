lShadow1=float(input('enter length of object : '))
height1=float(input('enter height of object : '))
lShadow2=float(input('enter length of shadow of object 2: '))
hObject2=(height1*lShadow2)/lShadow1

print('The height of object 2 is: ',round(hObject2,2))
